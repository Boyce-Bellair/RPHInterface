        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InterfacePrototype"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="North" Content="North" HorizontalAlignment="Left" Margin="443,156,0,0" VerticalAlignment="Top" Click="North_Click"/>
        <Button x:Name="West" Content="West" HorizontalAlignment="Left" Margin="368,201,0,0" VerticalAlignment="Top"/>
        <Button x:Name="East" Content="East" HorizontalAlignment="Left" Margin="503,201,0,0" VerticalAlignment="Top"/>
        <Button x:Name="South" Content="South" HorizontalAlignment="Left" Margin="443,246,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="GameText" HorizontalAlignment="Left" Margin="647,201,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
